{"ast":null,"code":"import { isNullOrWhiteSpace, joinNotEmpty, replaceAll } from './stringHelper';\nexport var getBaseUrl = function getBaseUrl(url) {\n  var result = null;\n\n  if (!isNullOrWhiteSpace(url)) {\n    result = url;\n    var splitIdx = result.indexOf('?');\n\n    if (splitIdx !== -1) {\n      result = result.substring(0, splitIdx);\n    }\n  }\n\n  return result;\n};\nexport var getFriendlyUrlSegment = function getFriendlyUrlSegment(value) {\n  if (value == null || value == undefined) {\n    return value;\n  }\n\n  var result = value.toString().toLowerCase();\n  result = replaceAll(result, \" \", \"-\");\n  result = replaceAll(result, \",\", \"\");\n  result = replaceAll(result, \".\", \"\");\n  result = replaceAll(result, \"#\", \"\");\n  result = replaceAll(result, \"'\", \"\");\n  result = replaceAll(result, \"\\\"\", \"\");\n  result = replaceAll(result, \"&\", \"\");\n  result = replaceAll(result, \"%\", \"\");\n  result = replaceAll(result, \"!\", \"\");\n  result = replaceAll(result, \"\\\"\", \"\");\n  result = replaceAll(result, \"?\", \"\");\n  result = replaceAll(result, \"(\", \"\");\n  result = replaceAll(result, \")\", \"\");\n  result = replaceAll(result, \":\", \"-\");\n  result = replaceAll(result, \"�\", \"-\");\n  result = replaceAll(result, \"_\", \"-\");\n  result = replaceAll(result, \"��\", \"-\");\n  result = replaceAll(result, \"�\", \"\");\n  result = replaceAll(result, \"�\", \"\");\n  result = replaceAll(result, \"/\", \"\");\n  result = replaceAll(result, \"\\\\\", \"\");\n  result = replaceAll(result, \"[\", \"\");\n  result = replaceAll(result, \"]\", \"\");\n  result = replaceAll(result, \"+\", \"-\");\n\n  while (result.includes(\"--\")) {\n    result = replaceAll(result, \"--\", \"-\");\n  } // result = result.trim('-', '_')\n\n\n  return result;\n};\nexport var getUrl = function getUrl(baseUrl, queryParams) {\n  var result = null;\n\n  if (!isNullOrWhiteSpace(baseUrl)) {\n    result = baseUrl;\n\n    if (queryParams && queryParams.length > 0) {\n      result += (result.indexOf('?') === -1 ? '?' : '&') + joinNotEmpty(\"&\", queryParams);\n    }\n  }\n\n  return result;\n};\nexport var getQueryParams = function getQueryParams(queryStringParts) {\n  if (!queryStringParts || queryStringParts.length === 0) return {};\n  var result = {};\n\n  for (var i = 0; i < queryStringParts.length; ++i) {\n    var param = queryStringParts[i].split('=', 2);\n\n    if (param.length === 1) {\n      result[param[0].toLowerCase()] = \"\";\n    } else {\n      result[param[0].toLowerCase()] = decodeURIComponent(param[1].replace(/\\+/g, \" \"));\n    }\n  }\n\n  return result;\n};","map":{"version":3,"sources":["E:/current_project/PPH/tfs/Website/website/helpers/urlsHelper.tsx"],"names":["isNullOrWhiteSpace","joinNotEmpty","replaceAll","getBaseUrl","url","result","splitIdx","indexOf","substring","getFriendlyUrlSegment","value","undefined","toString","toLowerCase","includes","getUrl","baseUrl","queryParams","length","getQueryParams","queryStringParts","i","param","split","decodeURIComponent","replace"],"mappings":"AAAA,SAASA,kBAAT,EAA6BC,YAA7B,EAA2CC,UAA3C,QAA6D,gBAA7D;AAEA,OAAO,IAAMC,UAAU,GAAG,SAAbA,UAAa,CAACC,GAAD,EAAyB;AAC/C,MAAIC,MAAc,GAAG,IAArB;;AAEA,MAAI,CAACL,kBAAkB,CAACI,GAAD,CAAvB,EAA8B;AAC1BC,IAAAA,MAAM,GAAGD,GAAT;AACA,QAAME,QAAQ,GAAGD,MAAM,CAACE,OAAP,CAAe,GAAf,CAAjB;;AAEA,QAAID,QAAQ,KAAK,CAAC,CAAlB,EAAqB;AACjBD,MAAAA,MAAM,GAAGA,MAAM,CAACG,SAAP,CAAiB,CAAjB,EAAoBF,QAApB,CAAT;AACH;AACJ;;AAED,SAAOD,MAAP;AACH,CAbM;AAeP,OAAO,IAAMI,qBAAqB,GAAG,SAAxBA,qBAAwB,CAACC,KAAD,EAA2B;AAC5D,MAAIA,KAAK,IAAI,IAAT,IAAiBA,KAAK,IAAIC,SAA9B,EAAyC;AACrC,WAAOD,KAAP;AACH;;AAED,MAAIL,MAAc,GAAGK,KAAK,CAACE,QAAN,GAAiBC,WAAjB,EAArB;AACAR,EAAAA,MAAM,GAAGH,UAAU,CAACG,MAAD,EAAS,GAAT,EAAc,GAAd,CAAnB;AACAA,EAAAA,MAAM,GAAGH,UAAU,CAACG,MAAD,EAAS,GAAT,EAAc,EAAd,CAAnB;AACAA,EAAAA,MAAM,GAAGH,UAAU,CAACG,MAAD,EAAS,GAAT,EAAc,EAAd,CAAnB;AACAA,EAAAA,MAAM,GAAGH,UAAU,CAACG,MAAD,EAAS,GAAT,EAAc,EAAd,CAAnB;AACAA,EAAAA,MAAM,GAAGH,UAAU,CAACG,MAAD